#include "libft.h"
#include <stdlib.h>

char *ft_strtrim(char const *s1, char const *set)
{
	int		i;
	int		n;
	int		t;
	char	*s;

	i = ft_strlen(s1) - 1;
	n = 0;
	s = malloc(sizeof(char)*(i));
	t = 0;
	if (!s)
		return (NULL);
	while (set[n] && s1[i])
	{
		while (set[n] && )
		if (set[n] != s1[i] && i > 0)
		{
			i--;
			n++;
		}
		else
			s = ((char *)s1 + i);
	}
	return (s);
}
